package frocessing.geom 
{
	
	/**
	* Number3D
	* @author nutsu
	*/
	public class FNumber3D 
	{
		public var x:Number;
		public var y:Number;
		public var z:Number;
		
		public function FNumber3D( x:Number, y:Number, z:Number ) 
		{
			this.x = x;
			this.y = y;
			this.z = z;
		}
		
	}
	
}